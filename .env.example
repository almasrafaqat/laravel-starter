
APP_KEY=
# =========================
# Application Configuration
# =========================
APP_NAME="LaravelStarter"
APP_ENV=local
APP_DEBUG=true
APP_URL=http://laravel-starter.test/

# Sanctum configuration for SPA authentication
# Adjust the domains as per your frontend application URL
SANCTUM_STATEFUL_DOMAINS=localhost:3000
FRONTEND_URL=http://localhost:3000/



APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

# =========================
# Logging
# =========================
LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# =========================
# Database Configuration
# =========================
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=



# =========================
# Mail Configuration
# =========================
# For local development, you can use services like Mailtrap or Mailhog

MAIL_MAILER=
MAIL_HOST=
MAIL_PORT=
MAIL_USERNAME=
MAIL_PASSWORD=

# =========================
# Admin User (for initial setup)
# =========================
ADMIN_EMAIL=almas@gmail.com
ADMIN_PHONE=
ADMIN_PASSWORD=123456789





# =========================
# Session & Queue
# =========================
SESSION_DRIVER=file         # Use 'file' for local dev, 'database' or 'redis' for production
# SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null
# SESSION_DRIVER=database   # Uncomment to use database sessions

QUEUE_CONNECTION=redis     # Use 'sync' for local dev, 'redis' or 'database' for production
# QUEUE_CONNECTION=database

# =========================
# Cache
# =========================
CACHE_DRIVER=redis         # Use 'file' for local dev, 'redis' for production
CACHE_STORE=database
# CACHE_PREFIX=

# =========================
# Redis
# =========================
REDIS_CLIENT=predis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MEMCACHED_HOST=127.0.0.1

# =========================
# Broadcasting
# =========================
BROADCAST_CONNECTION=log   # Use 'log' for local, 'pusher', 'reverb', etc. for real-time

# =========================
# Filesystem
# =========================
FILESYSTEM_DISK=local

# =========================
# Mail
# =========================
# MAIL_MAILER=log            # Use 'log' for local, 'smtp' for real email
# MAIL_SCHEME=null
# MAIL_HOST=127.0.0.1
# MAIL_PORT=2525
# MAIL_USERNAME=null
# MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# =========================
# AWS (for S3, etc.)
# =========================
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# =========================
# Vite (Frontend Build Tool)
# =========================
VITE_APP_NAME="${APP_NAME}"

# =========================
# Google OAuth
# =========================
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_CALLBACK=http://localhost:8080/login/google/callback
GOOGLE_REDIRECT_URI=
